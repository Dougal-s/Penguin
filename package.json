{
  "name": "penguin",
  "version": "1.0.0",
  "description": "A basic sample browser",
  "repository": "https://github.com/Dougal-s/Penguin",
  "main": "main.js",
  "scripts": {
    "start": "electron ./src/main.js",
    "postinstall": "install-app-deps",
	"linux": "electron-packager ./src --platform=linux",
	"macos": "electron-packager ./src --platform=darwin --icon='./penguin-development.icns'",
	"windows": "electron-packager ./src --platform=win32"
  },
  "keywords": [
    "Audio"
  ],
  "author": "Dougal Stewart",
  "devDependencies": {
    "electron": "^8.2.5",
    "electron-builder": "^22.6.1",
    "electron-packager": "^14.2.1"
  },
  "dependencies": {
    "sqlite3": "^4.2.0"
  }
}
